    "primeflex": "^4.0.0",
    "primeicons": "^7.0.0",
    "primevue": "^4.3.3",
    "roboto-fontface": "*",
    "tiny-emitter": "^2.1.0",
    "vue": "^3.5.13",
    "vue-google-charts": "^1.1.0",
    "vue-intl": "^6.5.25",
    "vue-router": "^4.5.0",
    "vuex": "^4.0.2",
    "webfontloader": "^1.0.0",
